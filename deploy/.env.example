# Copy to: deploy/private/.env.local

# Windows absolute path (example: D:/srv/dungeonclaw)
HOST_DATA_ROOT=D:/srv/dungeonclaw

# App image (replace with your backend image once available)
APP_IMAGE=traefik/whoami:v1.11
APP_UPSTREAM=game-api:80

# Domain and TLS
APP_DOMAIN=game.example.com
ACME_EMAIL=ops@example.com

# Database
POSTGRES_DB=dungeonclaw
POSTGRES_USER=dungeonclaw
POSTGRES_PASSWORD=change-me-now

# Timezone
TZ=Asia/Seoul
